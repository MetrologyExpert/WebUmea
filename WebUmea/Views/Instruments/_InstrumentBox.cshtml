@model List<WebUmea.Models.DemoGroup<int,WebUmea.Models.Contribution>>

<table class="table">
    @*<thead><tr><th>Key</th><th>Title</th><th>Uncertainty Constributions</th></tr></thead>*@
    <tbody>
        @foreach (var group in Model)
    {
        <tr><th>Uncertainty Budget Id: @group.Key</th></tr>
        <tr>
            <th>ContributionId</th>
            <th>Name</th>
            <th>EstimatedValue</th>
            <th>PdfId</th>
            <th>SensitivityCoefficient</th>
            <th>StandardUncertainty</th>
        </tr>
    foreach (var item in group.Values)
    {
        <tr>
            <td>@item.ContributionId</td>
            <td>@item.Name</td>
            <td>@item.EstimatedValue</td>
            <td>@item.PdfId</td>
            <td>@item.SensitivityCoefficient</td>
            <td>@item.StandardUncertainty</td>
        </tr>
}
}
        @*</tbody>*@
</table>


